    "url": "https://github.com/ezze/rollup-watch-issue-demo/issues"
  },
  "homepage": "https://github.com/ezze/rollup-watch-issue-demo#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "chokidar": "^1.7.0",
    "rollup": "^0.52.1",
    "rollup-plugin-babel": "^3.0.2"
  }
